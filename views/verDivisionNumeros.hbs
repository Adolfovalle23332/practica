<h1 class="mt-3">Resultado - Division de Numeros</h1>

{{#if numeros}}
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Numero 1</th>
      <th scope="col">Numero 2</th>
    </tr>
  </thead>
  <tbody>
    {{#each numeros}}
    <tr>
      <th scope="row">{{n1}}</th>
      <th scope="row">{{n2}}</th>
    </tr>
    {{/each}}
  </tbody>
</table>
{{/if}}